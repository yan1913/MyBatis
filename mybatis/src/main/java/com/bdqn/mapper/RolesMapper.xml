<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bdqn.mapper.RolesMapper">

    <select id="selectRolesById" resultType="Roles" parameterType="java.util.Map">
        SELECT roleId,roleCode,roleName,remark FROM roles WHERE roleId=#{roleId}
    </select>
</mapper>